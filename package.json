{
  "name": "PS-discord-bot",
  "version": "0.0.0",
  "description": "PS-discord-bot",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/!(*.queries).ts": [
      "yarn prettier",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/jquery": "^3.5.4",
    "@types/jsdom": "^16.2.5",
    "@types/node": "^12.12",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.20.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jsdom": "^16.4.0",
    "lint-staged": "^10.5.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.2.0",
    "ts-node-dev": "^1.0.0",
    "tsutils": "^3.17.0",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "dev": "yarn prettier && ts-node-dev --poll --respawn --transpile-only src/main.ts --trace-sync-io",
    "test": "jest",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "prettier": "prettier --write --config .prettierrc ./src///*.ts && prettier --write --config .prettierrc ./test///*.ts",
    "build": "tsc",
    "prebuild": "rimraf ./dist"
  },
  "author": "CuteWisp <sweatpotato13@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@odanado/atcoder-api": "^0.0.2",
    "ajax": "^0.0.4",
    "codeforces-api": "^1.0.3",
    "jquery": "^3.5.1",
    "node-fetch": "^2.6.1",
    "tslib": "^2.0.1"
  },
  "volta": {
    "node": "12.18.3",
    "npm": "6.14.5"
  }
}
